<div id="jumbo" class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-3 text-white text-center">Get In Touch</h1>
        <p class="text-white text-center">Please feel free to send me a message!</p>
        <p id="res" class="text-white text-center"></p>
    </div>
</div>

<div class="container">
    <fieldset>
        <form>
            <div class="col-6">
                <div class="form-group">
                    <label for="name">Full Name</label>
                    <input id="name" type="text" class="form-control" name="myName" placeholder="John Doe">
                </div>
                <div class="form-group">
                    <label for="email">Email</label>
                    <input id="email" type="email" class="form-control" name="email" placeholder="john@email.com">
                </div>
                <div class="form-group">
                    <label for="message">Message</label>
                    <textarea id="message" class="form-control" name="message" rows="4" placeholder="Hi there..."></textarea>
                </div>
                <button type="submit" value="submit" class="btn btn-success" id="submit_button">Send</button>
                <script src="js/script.js"></script>
            </div>
        </form>
    </fieldset>
</div>
